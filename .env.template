# Agent Environment Configuration Template
# Copy this file to .env and fill in your actual API keys and credentials

# Context7 API Key
CONTEXT7_API_KEY=your_context7_api_key_here

# Stripe API Key
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Browserbase API Configuration
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# PostHog Authentication
POSTHOG_AUTH_HEADER=Bearer your_posthog_api_key_here

# OpenAI API Key (for brain-trust-ask-mcp)
OPENAI_API_KEY=your_openai_api_key_here

# Packages API Authentication Token
PACKAGES_API_AUTH_TOKEN=your_packages_api_token_here

# Claude Code Router Port
CLAUDE_CODE_ROUTER_PORT=your_port_here
